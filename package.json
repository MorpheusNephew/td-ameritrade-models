    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.1.2",
